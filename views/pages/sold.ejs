<% include ../partials/head %>
<table class="table">
    <thead>
        <tr>
        <th>ITEM NAME</th>
        <th>QUANTITY PURCHASED</th>
        <th>AMOUNT</th>
        </tr>
    </thead>
    <tbody>
    <% sold.forEach(function(sold) { %>
        <tr>
            <td scope="row"><font color="blue"><%= sold.title %></font></td>
            <td><font color="blue"><%= sold.quantity %></font></td>
            <td><font color="blue">$<%= sold.price %></font></td>
        </tr>
        <% }); %>
        <% sum.forEach(function(sum) { %>
        <tr>
            <td scope="row">Total</td>
            <td id="quantity"><%= sum.squantity %></td>
            <td id="price">$<%= sum.sprice %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
<% include ../partials/footer %>